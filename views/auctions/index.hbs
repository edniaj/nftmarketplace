{{#extends 'base'}}

{{#block 'content'}}
<table class="table">
    <thead>
        <tr>
            <th>Configuration</th>
            <th>id</th>
            <th>nft_id</th>
            <th>auctionGroup_id</th>
            <th>user_id</th>
            <th>minBidAmount</th>
            <th>bidInterval</th>
        </tr>

    </thead>
    <tbody>
        {{#each databaseData}}
        <tr>
            <td>
                <a href="/auctions/{{this.id}}/update" class="btn btn-primary btn-sm">Update</a>
                <a href="/auctions/{{this.id}}/delete" class="btn btn-danger btn-sm">Delete</a>
            </td>
            <td>
                {{id}}
            </td>
            <td>
                {{nft_id}}
            </td>
            <td>
                {{auctionGroup_id}}
            </td>
            <td>
                {{user_id}}
            </td>
            <td>
                {{minBidAmount}}
            </td>
            <td>
                {{bidInterval}}
            </td>

        </tr>
        {{/each}}
    </tbody>
</table>
{{/block}}

{{/extends}}